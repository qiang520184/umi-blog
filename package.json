{
  "private": true,
  "scripts": {
    "serve": "umi dev",
    "dev": "umi dev",
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@uiw/react-codemirror": "^2.2.1",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.1.3",
    "axios": "^0.19.2",
    "codemirror": "^5.53.2",
    "highlight.js": "^10.0.2",
    "lint-staged": "^10.0.7",
    "markdown-it": "^10.0.0",
    "markdown-it-deflist": "^2.0.3",
    "markdown-it-implicit-figures": "^0.10.0",
    "markdown-it-imsize": "^2.0.1",
    "markdown-it-ruby": "^0.1.1",
    "markdown-it-table-of-contents": "^0.4.4",
    "mathjax": "^3.0.5",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-aplayer": "^1.0.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.0.0",
    "umi": "^3.1.3",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@umijs/preset-ui": "^2.1.13"
  }
}
